{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "dist",
    "sourceMap": true,
    "strict": true,
    "target": "es2017",
    "lib": ["esnext", "dom", "dom.iterable", "scripthost"]
  },
  "compileOnSave": true,
  "exclude": ["dist"],
  "include": ["src"]
}
